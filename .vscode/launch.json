  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Program",
      "skipFiles": ["<node_internals>/**"],
      "program": "${workspaceFolder}\\src\\build.ts",
      "outFiles": ["${workspaceFolder}/**/*.js"]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Decode assembly for debug",
      "skipFiles": ["<node_internals>/**"],
      "program": "${workspaceFolder}\\src\\decodebin.ts",
      "outFiles": ["${workspaceFolder}/**/*.js"]
    }
  ]
}
